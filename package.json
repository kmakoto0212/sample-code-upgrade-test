{
  "name": "sample-code-upgrade-test",
  "version": "1.0.0",
  "description": "サンプルコードのテスト用リポジトリ",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "all-install": "npm-run-all -s \"prefix:* -- install\"",
    "all-update": "npm-run-all -p \"prefix:* -- update\"",
    "prefix:self": "npm --prefix ./",
    "prefix:typescript": "npm --prefix ./typescript"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kmakoto0212/sample-code-upgrade-test.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.8.1",
    "eslint": "7.13.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.2"
  },
  "bugs": {
    "url": "https://github.com/kmakoto0212/sample-code-upgrade-test/issues"
  },
  "homepage": "https://github.com/kmakoto0212/sample-code-upgrade-test#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
